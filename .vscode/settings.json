{
    // some of the longer integration tests with
    // gleaner can take up to 45 sec since they
    // require downloading remote data in a blocking
    // way twice
    "go.testTimeout": "45s",
    "gopls": {
        // Make local paths have intellisense relative to the
        // project's internal files, not remote docs 
        "ui.navigation.importShortcut": "Definition",
    },
    "rust-analyzer.cargo.buildScripts.rebuildOnSave": true,
}